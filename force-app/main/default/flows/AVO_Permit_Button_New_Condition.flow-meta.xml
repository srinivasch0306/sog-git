    <formulas>
        <name>specialConditionFormula</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Frost Protection Only&quot;, {!frostProtectionOnlyDefaultVar},
IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Associated Permit&quot;, {!associatedPermitDefault},
IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Low Flow Protection&quot;, {!lowFlowProtectionDefault},
IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Transmission Line Right-of-way&quot;, {!transmissionLineRightOfWayDefault},
IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Well Construction Condition&quot;, {!wellConstructionConditionDefault},
IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Frost Protection Dual Rate&quot;, {!frostProtectionDualRateDefaultVar},
IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Well Location Condition&quot;, {!wellLocationConditionDefault},
IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Flint Frost Inspection Condition&quot;, {!flintFrostInspectionConditionDefault},
IF(TEXT({!conditionVar.AVO_Special_Condition_Category__c}) = &quot;Drought Condition&quot;, {!droughtConditionDefault}, &quot;&quot;
)
)
)
)
)
)
)
)
)</expression>
    </formulas>
