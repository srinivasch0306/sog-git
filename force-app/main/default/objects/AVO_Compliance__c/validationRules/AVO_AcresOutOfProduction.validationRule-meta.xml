<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AVO_AcresOutOfProduction</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(AVO_AcresOutOfProduction__c )),
 AVO_AcresOutOfProduction__c &lt; 0
)</errorConditionFormula>
    <errorDisplayField>AVO_AcresOutOfProduction__c</errorDisplayField>
    <errorMessage>Value must be greater than or equal to 0.</errorMessage>
</ValidationRule>
