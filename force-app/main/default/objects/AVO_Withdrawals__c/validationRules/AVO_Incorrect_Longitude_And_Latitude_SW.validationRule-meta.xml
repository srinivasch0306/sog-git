<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AVO_Incorrect_Longitude_And_Latitude_SW</fullName>
    <active>false</active>
    <errorConditionFormula>OR(
    Point_X__c  &gt; -80.75777,
    Point_X__c &lt; -85.60238,
    Point_Y__c &gt; 34.997296,    
    Point_Y__c &lt; 30.319872
)</errorConditionFormula>
    <errorDisplayField>Point_Y__c</errorDisplayField>
    <errorMessage>The longitude or latitude values are incorrect. Please enter values within the boundaries of the State of Georgia.</errorMessage>
</ValidationRule>
