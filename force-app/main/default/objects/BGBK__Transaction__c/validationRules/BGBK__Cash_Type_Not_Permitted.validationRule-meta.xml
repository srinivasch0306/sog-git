<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BGBK__Cash_Type_Not_Permitted</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISPICKVAL( BGBK__Cash_Type__c, &apos;&apos;)),  NOT(ISPICKVAL(BGBK__Type__c, &apos;Cash&apos;)))</errorConditionFormula>
    <errorMessage>Cash Type can only be specified for Cash Transactions</errorMessage>
</ValidationRule>
