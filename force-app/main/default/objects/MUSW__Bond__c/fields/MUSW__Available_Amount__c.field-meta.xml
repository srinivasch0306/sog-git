<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MUSW__Available_Amount__c</fullName>
    <deprecated>false</deprecated>
    <description>Available amount on a bond for distribution</description>
    <formula>IF(  ISPICKVAL( MUSW__Status__c , &quot;Posted&quot;) , MUSW__BG_Amount__c -  MUSW__Distributed_Amount__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Available Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
