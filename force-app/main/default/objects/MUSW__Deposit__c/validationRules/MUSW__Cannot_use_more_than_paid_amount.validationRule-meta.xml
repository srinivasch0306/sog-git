<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MUSW__Cannot_use_more_than_paid_amount</fullName>
    <active>true</active>
    <errorConditionFormula>MUSW__Used_Amount__c &gt; (MUSW__Paid_Amount__c + MUSW__Transfered_In_Amount__c -  MUSW__Transfered_Out_Amount__c  )</errorConditionFormula>
    <errorDisplayField>MUSW__Used_Amount__c</errorDisplayField>
    <errorMessage>Cannot use more than Paid Amount</errorMessage>
</ValidationRule>
