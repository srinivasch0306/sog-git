<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MUSW__Currency_Enforce_Decimal_Places</fullName>
    <active>true</active>
    <errorConditionFormula>ROUND(MUSW__Amount__c ,2) != MUSW__Amount__c</errorConditionFormula>
    <errorDisplayField>MUSW__Amount__c</errorDisplayField>
    <errorMessage>This is a currency field and can not have more than two decimal places</errorMessage>
</ValidationRule>
