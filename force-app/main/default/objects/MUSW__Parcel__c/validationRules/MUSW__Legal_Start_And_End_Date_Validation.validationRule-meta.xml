<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MUSW__Legal_Start_And_End_Date_Validation</fullName>
    <active>true</active>
    <description>Legal start date must be before legal end date.</description>
    <errorConditionFormula>AND(NOT(ISBLANK(MUSW__Legal_Start_Date__c)), NOT(ISBLANK(MUSW__Legal_End_Date__c)), MUSW__Legal_Start_Date__c &gt;= MUSW__Legal_End_Date__c)</errorConditionFormula>
    <errorMessage>Legal start date must be before legal end date.</errorMessage>
</ValidationRule>
