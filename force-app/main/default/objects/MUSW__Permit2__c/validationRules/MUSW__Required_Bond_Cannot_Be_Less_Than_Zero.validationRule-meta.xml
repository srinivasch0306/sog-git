<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MUSW__Required_Bond_Cannot_Be_Less_Than_Zero</fullName>
    <active>false</active>
    <errorConditionFormula>IF(ISBLANK( MUSW__Required_Bond_Amount__c ), false, MUSW__Required_Bond_Amount__c &lt;0)</errorConditionFormula>
    <errorDisplayField>MUSW__Required_Bond_Amount__c</errorDisplayField>
    <errorMessage>Required Bond Amount cannot be less than $0.00</errorMessage>
</ValidationRule>
