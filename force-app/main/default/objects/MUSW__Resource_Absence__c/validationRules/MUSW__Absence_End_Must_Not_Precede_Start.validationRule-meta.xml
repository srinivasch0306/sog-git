<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MUSW__Absence_End_Must_Not_Precede_Start</fullName>
    <active>true</active>
    <errorConditionFormula>MUSW__Absence_End_DateTime__c &lt;  MUSW__Absence_Start_DateTime__c</errorConditionFormula>
    <errorDisplayField>MUSW__Absence_End_DateTime__c</errorDisplayField>
    <errorMessage>Absence End must be later than or equal to Absence Start.</errorMessage>
</ValidationRule>
