<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>clariti__StartDateIsGreaterThanEndDate</fullName>
    <active>true</active>
    <description>End date can not occur on or before the start date</description>
    <errorConditionFormula>clariti__StartDate__c &gt;=  clariti__EndDate__c</errorConditionFormula>
    <errorDisplayField>clariti__EndDate__c</errorDisplayField>
    <errorMessage>End date can not occur on or before the start date.</errorMessage>
</ValidationRule>
