/* LWC SSR Client Utils v8.17.0-alpha.1 */
!function(){"use strict";const e=new Map;class t extends HTMLElement{connectedCallback(){const t=this.getAttribute("style-id");if(!t)throw new Error('"style-id" attribute must be supplied for <lwc-style> element');const s=this.getRootNode();let n=e.get(t);if(n){s.adoptedStyleSheets.push(n);const e=document.createElement("style");e.setAttribute("type","text/css"),this.replaceWith(e)}else{n=new CSSStyleSheet;const l=s.getElementById(t);if(!l)throw new Error(`<lwc-style> tag found with no corresponding <style id="${t}"> tag`);n.replaceSync(l.innerHTML),e.set(t,n),this.remove()}}}customElements.define("lwc-style",t)}();
